{
  "manifest_version": 3,
  "name": "MyTab",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "favicon-16x16.png",
      "32": "favicon-32x32.png",
      "192": "android-chrome-192x192.png",
      "512": "android-chrome-512x512.png"
    }
  },
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "192": "android-chrome-192x192.png",
    "512": "android-chrome-512x512.png"
  }
}
